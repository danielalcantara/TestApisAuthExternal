package br.com.cea.util;

public class StringUtil {

	public static String espaco(String string){
		String retorno = "";
		if (string.length() == 30){
			retorno = ": ";
		}
		
		if (string.length() < 30){
			for (int i = string.length(); i < 30; i++){
				retorno += ".";
			}
			retorno += ": ";
		}
		return retorno;
	}
}
